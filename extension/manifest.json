{
  "manifest_version": 3,
  "name": "Sarangsh CORS Helper",
  "version": "1.0",
  "description": "Handle CORS for Sarangsh application",
  "permissions": [
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "https://api.anthropic.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
